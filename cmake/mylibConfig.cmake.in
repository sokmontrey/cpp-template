@PACKAGE_INIT@

# Define variables that users might need to configure
set(MYLIB_INCLUDE_DIRS "@PACKAGE_INCLUDE_DIRS@")
set(MYLIB_LIBRARIES "@PACKAGE_LIBRARIES@")

# Add targets for imported library
add_library(${PROJECT_NAME}::${PROJECT_NAME} INTERFACE IMPORTED GLOBAL)
target_include_directories(${PROJECT_NAME}::${PROJECT_NAME} INTERFACE ${MYLIB_INCLUDE_DIRS})
target_link_libraries(${PROJECT_NAME}::${PROJECT_NAME} INTERFACE ${MYLIB_LIBRARIES})
